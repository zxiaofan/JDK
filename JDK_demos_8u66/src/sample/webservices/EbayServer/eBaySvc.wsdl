						<CallInfo>
							<CallName>GetCategories</CallName>
							<Returned>Conditionally</Returned>
						</CallInfo>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="LSD" type="xs:boolean" minOccurs="0">
