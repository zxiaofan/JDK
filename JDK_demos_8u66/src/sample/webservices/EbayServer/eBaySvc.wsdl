	<xs:element name="SetPromotionRulesRequest" type="ns:SetPromotionRulesRequestType"/>
	<xs:complexType name="SetPromotionRulesRequestType">
		<xs:annotation>
			<xs:documentation>
			 Requests to define cross-promotion rules for an item
			 or store category.
			</xs:documentation>
