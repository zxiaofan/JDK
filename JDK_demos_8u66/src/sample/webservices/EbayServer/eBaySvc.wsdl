					<xs:appinfo>
						<CallInfo>
							<CallName>GetFeedback</CallName>
							<Returned>Conditionally</Returned>
					</CallInfo>
				</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:any processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
