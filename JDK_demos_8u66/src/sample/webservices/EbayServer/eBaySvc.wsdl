							<xs:appinfo>
								<CallInfo>
									<CallName>SetUserPreferences</CallName>
									<RequiredInput>No</RequiredInput>
								</CallInfo>
							</xs:appinfo>
						</xs:annotation>
					</xs:element>
					<xs:element name="eBxOptInPreference" type="ns:eBxOptInPreferenceType" minOccurs="0">
						<xs:annotation>
